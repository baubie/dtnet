<?xml version="1.0" ?>

<!--
Each trial can have a maximum of ONE range over duration.
Each trial can have a maximum of ONE range over delay.
Each trial can have any number of inputs
-->

<trial>
    <input type="pure" ID="main">
        <param name="delay">0</param>
        <param name="amplitude">1</param>
        <param name="duration"><range start="0" end="60" step="2" /></param>
    </input>
</trial>
